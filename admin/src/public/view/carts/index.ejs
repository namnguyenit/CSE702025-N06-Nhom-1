<%- include('../partials/header') %>
    <main>
        <h1>Carts index</h1>
        <table>
            <thead>
                <tr>
                    <td>Name</td>
                    <td>Image</td>
                </tr>
            </thead>
            <tbody>
                <% products.forEach(item=>{ %>
                    <tr>
                        <td>
                            <%=item.name %>
                        </td>
                        <td>
                            <img class="image" src="/carts/image/<%=item.id %>" alt="<%=item.image.imageName%>">
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </main>
    <%- include('../partials/footer') %>